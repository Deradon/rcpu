program :main do
  # 1 word, filled with zeros
  data :word, 1
  # 3 words, filled with [1, 2, 3]
  data :bytestring, [1, 2, 3]
  # 6 words, filld with "hello\0"
  data :string, "hello\0".bytes

  block :init do
    SET a, [:word]
    SET b, [:bytestring]
    SET c, [:string]
    jump :crash
  end

  block :crash do
    jump :crash
  end
end

