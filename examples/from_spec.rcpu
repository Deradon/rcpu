program :main do
  data :test, 1
  data :loopdata, 10

  block :init do
    SET a, 0x30
    SET [:test], 0x20
    SUB a, [:test]
    IFN a, 0x10
      SET pc, :crash
  end

  block :start_loop do
    SET i, 10
    SET a, :loopdata
  end

  block :loop do
    SET [i + :loopdata], [a]
    SUB i, 1
    IFN i, 0
      SET pc, :loop
  end

  block :callsub do
    SET x, 0x4
    JSR :testsub
    SET pc, :crash
  end

  block :testsub do
    SHL x, 4
    SET pc, pop
  end

  block :crash do
    SET pc, :crash
  end
end

